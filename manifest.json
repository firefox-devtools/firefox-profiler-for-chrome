{
  "manifest_version": 3,
  "name": "Firefox Profiler",
  "version": "1.0",
  "permissions": ["activeTab", "scripting", "tabs", "debugger"],
  "host_permissions": [
    "https://profiler.firefox.com/*",
    "http://localhost:4242/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {}
}
